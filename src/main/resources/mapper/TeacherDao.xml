<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.guomz.MyMOOC.dao.TeacherDao">

	<select id="queryTeacherById" parameterType="Long" resultType="Teacher">
		select *
		from teacher
		where
		teacher_id=#{0}
	</select>
	
</mapper>